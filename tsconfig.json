{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es2021",
    "types": ["node", "jest"],
    "sourceMap": true,
    "rootDir": "./typescript",
    "outDir": "./dist/",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  },
  "exclude": [
      "node_modules",
      ".idea",
      "brand-label-images",
      "images",
      "login",
      "website-images"
  ]
}